public class Funkcje {
	
	public static void main(String args[]){
		wypisz();
		System.out.println("Wartosc tuńczyka wynosi: "+tunczyk());
		System.out.println("Wartosc sumy liczb 10 i 20 wynosi: "+suma(10, 20));
		
		double promienKola = Math.round(Math.random()*100)/100.0;
		System.out.println("Obwód koła o promieniu: "+promienKola+" wynosi: "+
				Matn.round(obliczObwodKola(promienKola)*100)/100.0);
	}
	
	public static void wypisz(){
		System.out.println("Ta funkcja nic nie zwraca.\n" +
				"a ja oglądam bardzo nundy kurs.");
	}
	
	public static int tunczyk(){
		return 10;
	}
	
	public static int suma(int a, int b){
		return a+b;
	}
	
	public static double obliczObwodKola(double promien){
		return promien*2*Math.PI;
	}

}
