
public class Figura {
	
	String kolor;
	boolean wypelnienie;
	
	public Figura(String k, boolean w){
		kolor = k;
		wypelnienie = w;
	}
	
	public Figura(String k){
		this(k, false);
	}
	
	public Figura(boolean w){
		this("Czarny", w);
	}
	
	public Figura(){
		this("Czarny", false);
	}
	
	public String toString(){
		return "Kolor: "+kolor+", wypelnienie: "+wypelnienie;
	}

}
