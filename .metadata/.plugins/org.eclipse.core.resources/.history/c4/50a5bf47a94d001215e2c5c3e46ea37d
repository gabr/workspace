
public class Przekazywanie {
	
	public static void podwojenie(int x){
		x *= 2;
	}
	
	public static void podwojenie_tab(int[] t){
		for(int i = 0; i < t.length ; i++)
			t[i] *= 2;
	}
	
	public static void main(String[] args){
		int x = 10;
		podwojenie(x);
		System.out.println(x);
		
		int[] t = new int[1];
		t[0] = 10;
		podwojenie_tab(t);
		System.out.println(t);
	}

}
